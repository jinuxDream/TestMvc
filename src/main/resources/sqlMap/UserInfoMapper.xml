<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jyp.dao.UserInfoMapper">

    <select id="getUserList" resultType="com.jyp.dto.userInfoDTO">
        select user_id as userId,user_name as userName FROM user_info limit 10
    </select>

</mapper>